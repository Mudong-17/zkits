{
  "name": "@zkits/prettier",
  "version": "1.1.1",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "暮冬拾柒",
    "url": "https://github.com/Mudong-17/zkits"
  },
  "exports": {
    "./base": {
      "types": "./src/base.ts",
      "default": "./dist/base.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "up:latest": "pnpm up --latest"
  },
  "dependencies": {
    "prettier": "~3.6.2",
    "prettier-plugin-multiline-arrays": "~4.0.3",
    "prettier-plugin-organize-imports": "~4.2.0",
    "prettier-plugin-packagejson": "~2.5.19",
    "prettier-plugin-prisma": "~5.0.0",
    "prettier-plugin-tailwindcss": "~0.6.14"
  },
  "packageManager": "pnpm@10.5.2",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "format": [
      "cjs"
    ],
    "clean": true,
    "minify": true,
    "treeshake": true,
    "dts": false,
    "noExternal": [],
    "target": "node16"
  }
}
